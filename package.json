{
  "name": "key-util",
  "displayName": "Key Util",
  "description": "Utility tool for keybindings.json",
  "version": "0.0.1",
  "publisher": "hw104",
  "license": "MIT",
  "repository": "https://github.com/hw104/vscode-key-util.git",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:key-util.sort",
    "onCommand:key-util.info",
    "onCommand:key-util.mergeCmdAlt"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "key-util.sort",
        "shortTitle": "Sort keybindings.json",
        "title": "Key Util: Sort keybindings.json"
      },
      {
        "command": "key-util.info",
        "shortTitle": "Info keybindings.json",
        "title": "Key Util: Info keybindings.json"
      },
      {
        "command": "key-util.mergeCmdAlt",
        "shortTitle": "Merge Cmd and Alt",
        "title": "Key Util: Merge Cmd and Alt"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "vscode:package": "vsce package --yarn",
    "vscode:publish": "vsce publish --yarn",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.70.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "vsce": "^2.10.0"
  },
  "dependencies": {
    "jsonc-parser": "^3.1.0"
  }
}
